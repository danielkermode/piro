{
  "name": "piro",
  "version": "1.0.0",
  "description": "A scoring app.",
  "main": "index.js",
  "engines": {
    "node": "6.2.1"
  },
  "scripts": {
    "start": "node ./server/bin/www",
    "start:watch": "nodemon ./server/bin/www",
    "test": "cd client && babel-tape-runner test/*.js | tap-spec",
    "build": "cd client && webpack --progress --profile --colors",
    "build:watch": "cd client && webpack --progress --profile --colors --watch",
    "dev": "cd client && webpack-dev-server --hot --inline",
    "lint": "eslint . -c ./.eslintrc.json"
  },
  "keywords": [
    "Piro",
    "scoring"
  ],
  "author": "Piro",
  "license": "MIT",
  "dependencies": {
    "knex": "^0.11.9",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "passport": "^0.3.2",
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-tape-runner": "^2.0.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.1.1",
    "react-addons-test-utils": "^15.2.1",
    "sqlite3": "^3.1.4",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
